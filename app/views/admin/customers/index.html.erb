<h1>会員ID</h1>

<table class="table">
  <thead>
    <tr>
      <th>会員ID</th>
      <th>氏名</th>
      <th>メールアドレス</th>
      <th>ステータス</th>
    </tr>
  </thead>
  <tbody>
    <%= @customers.each do |customers| %>
      <tr>
        <td><%= customers.customers_id %></td>
        <td>
          <%= customers.last_name %>
          <%= customers.first_name %>
        </td>
        <td><%= customers.email %></td>
        <td><%= customers.is_deleted %></td>
      </tr>
    <% end %>
  </tbody>
</table>